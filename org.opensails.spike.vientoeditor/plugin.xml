<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.eclipse.core.runtime.contentTypes">
      <content-type
            file-extensions="vto"
            id="viento"
            name="Viento Files"
            priority="normal"/>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.opensails.spike.vientoeditor.VientoEditor"
            contributorClass="org.opensails.spike.vientoeditor.ActionBarContributor"
            default="true"
            extensions="vto"
            icon="icons/file_obj.gif"
            id="org.opensails.spike.vientoeditor.editor"
            name="Viento Editor">
         <contentTypeBinding contentTypeId="viento"/>
      </editor>
   </extension>

</plugin>
